{
  "name": "webthon",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/jobs/worker.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:worker\"",
    "start": "node dist/index.js",
    "start:worker": "node dist/jobs/worker.js",
    "go": "docker compose up -d && concurrently -n server,worker -c blue,green \"npx tsx src/index.ts\" \"npx tsx src/jobs/worker.ts\"",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeviPrasad7/Webthon.git"
  },
  "keywords": [],
  "author": "",
